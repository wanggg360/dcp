<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd">
	<localRepository>D:\software\repo</localRepository>
	<pluginGroups/>
	<proxies/>
	<servers>
		<server>
			<id>CRM-Snapshots</id>
			<username>chenlanhong</username>
			<password>JTXTsmartoa666</password>
		</server>
		<server>
			<id>Architect-Snapshot</id>
			<username>chenlanhong</username>
			<password>JTXTsmartoa666</password>
		</server>
		<server>
			<id>Architect</id>
			<username>chenlanhong</username>
			<password>JTXTsmartoa666</password>
		</server>
	</servers>
	<mirrors>
		<mirror>
			<id>alimaven</id>
			<name>aliyun maven</name>
			<!-- https://maven.aliyun.com/repository/public/ -->
			<url>http://maven.aliyun.com/nexus/content/groups/public/</url>
			<mirrorOf>central</mirrorOf>
		</mirror>
	</mirrors>
	<profiles>
		<profile>
			<id>jdk-1.8</id>
			<activation>
				<activeByDefault>true</activeByDefault>
				<jdk>1.8</jdk>
			</activation>
			<properties>
				<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
				<maven.compiler.source>1.8</maven.compiler.source>
				<maven.compiler.target>1.8</maven.compiler.target>
				<maven.compiler.compilerVersion>1.8</maven.compiler.compilerVersion>
			</properties>
			<repositories>
				
				<!-- 公共组仓库 -->
				<repository>
					<id>HT-Group</id>
					<name>HT Group</name>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<url>http://repo.htzq.htsc.com.cn/nexus/content/groups/HTSC-Group/</url>
					<layout>default</layout>
				</repository>
				
				<!-- 架构组仓库 -->
				<repository>
					<id>Architect</id>
					<name>Architect</name>
					<url>http://repo.htzq.htsc.com.cn/nexus/content/repositories/Architect/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>Architect-Snapshot</id>
					<name>Architect Snapshot Central</name>
					<url>http://repo.htzq.htsc.com.cn/nexus/content/repositories/Architect-Snapshot/</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				
				<!-- CRM组仓库 -->
				<repository>
					<id>HT-CRM</id>
					<name>HT-CRM</name>
					<url>http://repo.htzq.htsc.com.cn/nexus/content/repositories/CRM-Snapshots/</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>HT-StarCraft</id>
					<name>HT StarCraft</name>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
					</releases>
					<url>http://repo.htzq.htsc.com.cn/nexus/content/repositories/starcraft/</url>
					<layout>default</layout>
				</repository>
				<repository>
					<id>HT-3RD</id>
					<name>HT ThirdParty</name>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
					</releases>
					<url>http://repo.htzq.htsc.com.cn/nexus/content/repositories/thirdparty/</url>
					<layout>default</layout>
				</repository>
			</repositories>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>jdk-1.8</activeProfile>
	</activeProfiles>
</settings>
